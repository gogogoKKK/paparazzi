#include "autopilot.h"
inline void setNav(void){
autopilot_mode_auto2 = AP_MODE_NAV;
autopilot_static_set_mode(AP_MODE_NAV);
}
inline void setGuided(void){
autopilot_mode_auto2 = AP_MODE_GUIDED;
autopilot_static_set_mode(AP_MODE_GUIDED);
}
